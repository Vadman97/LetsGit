<h2>
  <%= project.name %>
</h2>
<div class="jumbotron breadcrumbs">
  <a href="#">first</a> > <a href="#">second</a> > current_folder
</div>

<!-- Main component for a primary marketing message or call to action -->
<div class="jumbotron">

  <div class="table-responsive well">
    <table class="table table-striped table-hover repoTable">
      <thead>
        <tr>
          <th><h4>File name</h4></th>
          <th><h4>Last commit</h4></th>
          <th><h4>Last updated</h4></th>
        </tr>
      </thead>
      <tbody>
        <% if(parent) { %>
          <tr>
            <td colspan="4"><a href=".."><span class="fa fa-level-up">&nbsp;</span>Parent</a></td>
          </tr>
        <% } %>
        <% for (index in files) { %>
          <tr>
            <td><a href="<%= currentPath + '/' + files[index] %>"><%= files[index] %></a></td>
            <td>ipsum</td>
            <td>dolor</td>
            <td>
              <a href="#" class="btn btn-warning" data-toggle="tooltip" data-placement="top" title="Rename"><span class="fa fa-pencil fa-lg"></span></a>
              <a href="#" class="btn btn-danger" data-toggle="tooltip" data-placement="top" title="Delete"><span class="fa fa-trash-o fa-lg"></span></a>
            </td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </div>
  <div class="row">
    <a href="/uploadRepo" class="btn btn-primary addButton">
      <span class="glyphicon glyphicon-upload"></span>
            Upload file
    </a>
    <form enctype="multipart/form-data" id="uploadZip">
      <input type="file">
      <input type="submit">
    </form>
    <a href="/commitRepo" class="btn btn-primary addButton">
      <span class="glyphicon glyphicon-file"></span>
            Create new file
    </a>
  </div>
</div>